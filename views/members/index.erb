<div class="info-backing">
  <h2>Here is a current list of Members</h2>
<div>
  <p><a href="/members/new">Click here to join our Society</a></p>
</div>
  <% for member in @members do %>
<div class="form-norm">
  <a href="/members/<%=member.id%>"><div class="profile-pic ani-pic" style="background-image: url('<%=member.picture%>')"></div></a>
  <p>Name:<%=member.name%></p>

  <p><a href="/members/<%=member.id%>">More details</a></p>
</div>
  <% end %>
</div>
